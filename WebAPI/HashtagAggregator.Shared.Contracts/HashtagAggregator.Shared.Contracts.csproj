    <AssemblyName>HashtagAggregator.Shared.Contracts</AssemblyName>
    <PackageId>HashtagAggregator.Shared.Contracts</PackageId>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
